<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.102"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Text fx:id="activityName" BorderPane.alignment="CENTER" />
    </top>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="10.0" right="10.0" />
            </BorderPane.margin>
            <ListView fx:id="availableActions" prefHeight="200.0" prefWidth="200.0"/>
            <ListView fx:id="pickedActions" prefHeight="200.0" prefWidth="200.0"/>
        </VBox>
    </center>
</BorderPane>
